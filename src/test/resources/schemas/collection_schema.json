{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "elapsedMilliseconds": { "type": "integer" },
    "count": { "type": "integer" },
    "countFacets": {
      "type": "object",
      "properties": {
        "hasimage": { "type": "integer" },
        "ondisplay": { "type": "integer" }
      },
      "required": ["hasimage", "ondisplay"]
    },
    "artObjects": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "links": {
            "type": "object",
            "properties": {
              "self": { "type": "string", "format": "uri" },
              "web": { "type": "string", "format": "uri" }
            },
            "required": ["self", "web"]
          },
          "id": { "type": "string" },
          "objectNumber": { "type": "string" },
          "title": { "type": "string" },
          "hasImage": { "type": "boolean" },
          "principalOrFirstMaker": { "type": "string" },
          "longTitle": { "type": "string" },
          "showImage": { "type": "boolean" },
          "permitDownload": { "type": "boolean" },
          "webImage": {
            "type": "object",
            "properties": {
              "guid": { "type": "string" },
              "offsetPercentageX": { "type": "integer" },
              "offsetPercentageY": { "type": "integer" },
              "width": { "type": "integer" },
              "height": { "type": "integer" },
              "url": { "type": "string", "format": "uri" }
            },
            "required": ["guid", "width", "height", "url"]
          },
          "headerImage": {
            "type": "object",
            "properties": {
              "guid": { "type": "string" },
              "offsetPercentageX": { "type": "integer" },
              "offsetPercentageY": { "type": "integer" },
              "width": { "type": "integer" },
              "height": { "type": "integer" },
              "url": { "type": "string", "format": "uri" }
            },
            "required": ["guid", "width", "height", "url"]
          },
          "productionPlaces": {
            "type": "array",
            "items": { "type": "string" }
          }
        },
        "required": [
          "links", "id", "objectNumber", "title",
          "hasImage", "principalOrFirstMaker", "longTitle",
          "showImage", "permitDownload", "webImage", "headerImage",
          "productionPlaces"
        ]
      }
    },
    "facets": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "facets": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": { "type": "string" },
                "value": { "type": "integer" }
              },
              "required": ["key", "value"]
            }
          },
          "name": { "type": "string" },
          "otherTerms": { "type": "integer" },
          "prettyName": { "type": ["string", "integer"] }
        },
        "required": ["facets", "name", "otherTerms", "prettyName"]
      }
    }
  },
  "required": ["elapsedMilliseconds", "count", "countFacets", "artObjects", "facets"]
}